<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.sizzlingbites">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/chicken"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SizzlingBites">
        <activity android:name=".DashboardActivity"
            android:theme="@style/Theme.SizzlingBites2"></activity>
        <activity
            android:name=".IntroductioryActivity"
            android:theme="@style/Theme.SizzlingBites2">

        </activity>
        <activity android:name=".MainActivity"
            android:theme="@style/Theme.SizzlingBites2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>